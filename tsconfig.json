{
	"compilerOptions": {
		"alwaysStrict": true,
		"experimentalDecorators": true,
		"jsx": "preserve",
		"lib": ["es6", "es7", "dom"],
		"moduleResolution": "node",
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"strictNullChecks": true,
		"target": "es6",
		"types": ["inferno"]
	},
	"exclude": ["node_modules"]
}
